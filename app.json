{
  "expo": {
    "name": "Rize",
    "slug": "rize",
    "version": "2.1.1",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "rize",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.hytek.rize",
      "versionCode": 4,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
     
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "1d476dbb-bd8f-42d0-b3a7-06b5023193b4"
      },
      "permissions": ["NOTIFICATIONS"]
    }
  }
}
